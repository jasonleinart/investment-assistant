{
  "name": "agentic-investing-system",
  "version": "1.0.0",
  "description": "Agentic investing system with technical analysis and dashboard",
  "scripts": {
    "start:backend": "./start_backend.sh",
    "start:frontend": "./start_frontend.sh",
    "start:dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
} 